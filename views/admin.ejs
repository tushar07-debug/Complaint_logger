<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title></title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>

<body>
  <div class="container text-center">
    <h1 class="alert alert-primary mt-2">Admin Page</h1>
    <form method="get" action="">
      <div style="display: flex;">
      <% complaints.forEach(function(complaint){ %>
        <div class="card" style="width: 18rem;margin-left: 20px;">
          <div class="card-body">
            <% if (complaint.img_path) {%>
            <img class="card-img-top" src="<%= '/' + complaint.img_path %>"> </img>
            <% } %>
              <h5 class="card-title">Email: <%= complaint.email %></h5>
              <p class="card-text">User Name: <%= complaint.name %></p>
              <p class="card-text">Location: <%= complaint.location %></p>
              <p class="card-text">Complaint: <%= complaint.message %></p>
          </div>
        </div>
      <% }); %>
      </div>
      <button class="btn btn-primary mt-2" type="submit">Fetch Contact</button>
    </form>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    crossorigin="anonymous"></script>
</body>

</html>